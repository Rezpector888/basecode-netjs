  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "pnpm prisma generate --schema=./prisma-app/schema.prisma && pnpm nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "worker": "nest start  --entryFile worker/main",
    "worker:dev": "pnpm prisma generate --schema=./prisma-app/schema.prisma && pnpm nest start --watch --entryFile worker/main",
    "worker:debug": "nest start --debug --watch  --entryFile worker/main",
    "worker:prod": "node dist/worker/main",
    "start": "nest start  --entryFile api/main",
    "start:dev": "pnpm prisma generate --schema=./prisma-app/schema.prisma && pnpm nest start --watch --entryFile api/main",
    "start:debug": "nest start --debug --watch  --entryFile api/main",
    "start:prod": "node dist/api/main",
    "start:doc": "compodoc -p tsconfig.json -s",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json"
  },
  "dependencies": {
    "@itgorillaz/configify": "^4.0.2",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/throttler": "^6.5.0",
    "@prisma/adapter-mssql": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "body-parser": "^2.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.1",
    "helmet": "^8.1.0",
    "mssql": "^12.2.0",
    "nestjs-pino": "^4.5.0",
    "npm-check-updates": "^19.3.1",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "prisma-pagination": "^0.2.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.32",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.8",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/mssql": "^9.1.8",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.1",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "npm": ">=11.0.0",
    "node": ">=24.0.0"
  }
}
